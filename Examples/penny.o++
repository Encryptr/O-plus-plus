var day_number = 1;
var daily_amount = 0.01;
var sum = 0.0;

print("Day       Daily Amount     Total Amount");


while sum < 1000000.0 {
    sum = sum + daily_amount;
    echo(day_number, "       ", daily_amount, "     ", sum);
    
    daily_amount = daily_amount*2.0;
    day_number = day_number + 1;
}

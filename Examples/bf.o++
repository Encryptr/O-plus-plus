var mem = [0];
var in = [];
var ptr = 0;
var idx = 0;
var ch = "";

func analize(a) {
	if a == "+" mem[ptr] = mem[ptr] + 1;
	if a == "-" mem[ptr] = mem[ptr] - 1;
	if a == ">" { append(mem, 0); ptr = ptr + 1; }
	if a == "<" ptr = ptr - 1;
	if a == "." print(mem[ptr]);
	if a == "," mem = getc();
}

func parse() {
	while ch[idx] != "" {
		
		analize(ch[idx]);

		idx = idx + 1;
	}	
}

func main() {
	ch = input();
	parse();

}

main();
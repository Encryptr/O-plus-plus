func matrix_dot(mat1, mat2) 
    | mat1 != null and mat2 != null
    | len(mat2) > 0 and len(mat2) > 0
    | len(mat2) == len(mat1)
{
    sum = 0

    for i = 0, i < len(mat1), i++ {
        sum += mat1[i] * mat2[i]
    }

    return sum
}

